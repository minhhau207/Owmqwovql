_G.GoGat = true
spawn(function()
	while wait(0) do
		if _G.Start_Kaitun then
			MyLevel = game.Players.localPlayer.Data.Level.value
			if MyLevel >= 15 then
				for i,v in pairs(Code) do
					pcall(function()
						local args = {
							[1] = v
						}
						game:GetService("ReplicatedStorage").Remotes.Redeem:InvokeServer(unpack(args))
					end)
				end
				break
			end
		end
	end
end)

