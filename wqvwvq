			if game:GetService("Players").LocalPlayer.Data.Race:FindFirstChild("Evolved") then
				Race_Stast:Set("Race Stast V2 : ✅")
			else
				Race_Stast:Set("Race Stast V2 : ❌")
			end
		end
	end)
end)


